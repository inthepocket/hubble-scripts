{
  "name": "hubble-scripts",
  "version": "2.0.0",
  "engines": {
    "node": ">=8.0"
  },
  "bin": {
    "hubble-cli": "./cli.js",
    "hubble": "bash hubble.sh"
  },
  "scripts": {
    "start": "node cli.js",
    "start:document": "npm start -- --dump",
    "start:artboards": "npm start -- --dump --useColorArtboards --useGradientArtboards",
    "clean": "rm hubble-data.json && rm -rf sketchtool-export node_modules && npm install",
    "export": "./sketchtool.sh",
    "lint": "itp-react-scripts lint",
    "lint:fix": "npm run lint -- --fix",
    "lint:shell": "which shellcheck > /dev/null && shellcheck *.sh",
    "generate:mocks": "npm run start:artboards ./__mocks__/sample_sketchfile.sketch && mv hubble-data.json ./__mocks__/sample_output.json && mv logdump.json ./__mocks__/sample_dump.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "npm run test -- --updateSnapshot",
    "test:travis": "which docker > /dev/null && trevor",
    "build": "pkg cli.js -t node8-macos-x64 --output ./bin/hubble-cli"
  },
  "devDependencies": {
    "@inthepocket/itp-react-scripts": "^0.48.0",
    "eslint": "^5.10.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "pkg": "^4.3.5",
    "trevor": "^2.3.0"
  },
  "author": "Thibault Maekelbergh <thibault.maekelbergh@icloud.com>",
  "dependencies": {
    "camelcase": "^5.0.0",
    "color-convert": "^1.9.3",
    "color-namer": "^1.3.0",
    "meow": "^5.0.0",
    "sketch2json": "^0.1.2"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint:fix"
    ],
    "**/*.sh": [
      "npm run lint:shell"
    ]
  }
}
